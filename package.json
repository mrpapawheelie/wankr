{
  "name": "wankr",
  "version": "1.0.0",
  "description": "WANKR - The Dark Mirror of the Base Chain",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon src/server.ts",
    "dev:client": "cd frontend && npm run dev",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc",
    "build:client": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "clean": "rm -rf node_modules frontend/node_modules",
    "start": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alcb4/wankr.git"
  },
  "keywords": [
    "wankr",
    "crypto",
    "token",
    "base",
    "blockchain",
    "meme",
    "web3",
    "solidity",
    "react"
  ],
  "author": "WANKR Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alcb4/wankr/issues"
  },
  "homepage": "https://github.com/Alcb4/wankr#readme",
  "dependencies": {
    "@duneanalytics/client-sdk": "^0.2.5",
    "@neynar/nodejs-sdk": "^3.34.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "ethers": "^6.15.0",
    "express": "^5.1.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.33.0",
    "prettier": "^3.6.2"
  }
}
